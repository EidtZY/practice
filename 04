'''

print('随机整数0-10是：')
x = random.randint(0,10)
print(x)
'''
'''
a = 2.2
b = 9.9
c = random.uniform(a,b)
print('在%f-%f之中的随机数是：%f'%(a,b,c))
'''
import random
a = 'tomorrow'
b = random.choice(a)
print('在字符串%s中随机挑选的字符是：%s'%(a,b))
